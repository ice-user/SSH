{
  "manifest_version": 3,
  "name": "AlgoBot",
  "version": "1.0",
  "description": "Suggests relevant LeetCode problems based on user input.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://alfa-leetcode-api.onrender.com/*",
    "https://api.groq.com/*"
  ]
}